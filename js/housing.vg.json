{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Melbourne's Housing Markert",
  "layer": [
    {
      "data": {
        "url": "vic_localities.json",
        "format": {"type": "topojson", "feature": "ne_110m_graticules_1"}
      },
      "mark": {"type": "geoshape", "fill": null, "stroke": "black"}
    },
    {
      "data": {
        "url": "vic_localities.json",
        "format": {"type": "topojson", "feature": "vic_localities"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "../data/Melbourne_housing_FULL.csv"
      },
      "transform": [
        {"filter": "datum.Longtitude > 0"}
      ],
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "x": {
          "field": "suburb",
          "type": "ordinal",
          "scale": {
            "type": "band",
            "range": [0, 1]
          }
        },
        "size": {
          "aggregate": "mean",
          "field":"Price",
          "type": "quantitative"
        }
      }
    }
  ]
}


